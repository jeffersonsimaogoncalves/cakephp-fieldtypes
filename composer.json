{
	"name": "leoruhland/cakephp-fieldtypes",
	"description": "FieldTypes plugin for CakePHP. CakeAdmin compatible.",
	"license": "MIT",
	"type": "cakephp-plugin",
	"require": {
		"php": ">=5.4.16",
		"cakephp/cakephp": "~3.0",
		"holt59/cakephp3-bootstrap-helpers": "~3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "*"
	},
	"autoload": {
		"psr-4": {
			"FieldTypes\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"FieldTypes\\Test\\": "tests",
			"Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
		}
	}
}
