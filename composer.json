  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "FieldTypes\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FieldTypes\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
